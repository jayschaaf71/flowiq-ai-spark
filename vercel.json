{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/health"
    },
    {
      "src": "/chiropractic/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/dental-sleep/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],

  "env": {
    "VITE_ENVIRONMENT": "production"
  }
}